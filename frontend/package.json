    "@types/react-loadable": "^5.5.1",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/parser": "^1.5.0",
    "emotion": "^10.0.9",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-react-hooks": "^1.6.0",
