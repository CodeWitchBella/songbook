    "typescript": "^3.4.1"
  },
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "NODE_PATH=src react-scripts build",
    "test": "NODE_PATH=src react-scripts test",
    "eject": "NODE_PATH=src react-scripts eject",
    "lint": "eslint --ext ts --ext tsx --cache src",
    "az:docker": "docker run --rm -it -v $(pwd):/app -w /app -v $HOME/.azure:/root/.azure microsoft/azure-cli",
    "upload-docker": "npm run az:docker -- az storage blob upload-batch -s build/ -d '\\$web' --account-name songbook",
    "upload": "az storage blob upload-batch -s build/ -d \\$web --account-name songbook",
    "deploy": "npm run build && npm run upload-docker"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "workers-loader": "^2.1.2"
